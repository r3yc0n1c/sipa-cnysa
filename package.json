{
  "name": "sipa-cnysa",
  "version": "1.0.0",
  "description": "image processing csv api",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "dev:img-q-worker": "nodemon src/workers/imageQWorker.js",
    "prod:start-all": "npm run prod:start-img-q-worker && prod:start-server",
    "prod:stop-all": "npm run stop:start-server && npm run prod:stop-img-q-worker",
    "prod:start-server": "pm2 start src/index.js --name image-proc-api",
    "prod:stop-server": "pm2 stop image-proc-api",
    "prod:clean-server": "pm2 delete image-proc-api",
    "prod:start-img-q-worker": "pm2 start src/workers/imageQWorker.js --name image-q-worker",
    "prod:stop-img-q-worker": "pm2 stop image-q-worker",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Raja Majumdar (r3yc0n1c)",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.614.0",
    "axios": "^1.7.2",
    "bullmq": "^5.10.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.3.1",
    "fast-csv": "^5.0.1",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "mongoose": "^8.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.7",
    "pm2": "^5.4.2",
    "sharp": "^0.33.4",
    "winston": "^3.13.1",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
}
